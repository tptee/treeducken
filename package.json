{
  "name": "treeducken",
  "version": "1.0.0",
  "description": "Stuffing a bit of Elm into your Redux!",
  "main": "lib/index.js",
  "scripts": {
    "postinstall": "cd lib || npm run build",
    "preversion": "npm run check",
    "version": "npm run clean && npm run build",
    "clean": "rimraf lib",
    "build": "npm run clean && babel src -d lib --copy-files",
    "lint": "eslint --color --ext .js,.jsx src test demo/*.js*",
    "test": "mocha test 'test/**/*.spec.js*'",
    "coverage": "nyc --source-map false --instrument false mocha test 'test/**/*.spec.js*'",
    "check": "concurrently 'npm run lint' 'npm run test'",
    "demo": "webpack-dev-server --port 3000 --config config/webpack.config.dev.js --colors --content-base demo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://tptee@github.com/tptee/treeducken.git"
  },
  "keywords": [
    "redux",
    "elm",
    "fractal",
    "composition",
    "inception",
    "delicious"
  ],
  "author": "Tyler Thompson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tptee/treeducken/issues"
  },
  "homepage": "https://github.com/tptee/treeducken#readme",
  "dependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "coveralls": "^2.11.9",
    "lodash.isnumber": "^3.0.3",
    "recompose": "^0.20.2",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.15"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-plugin-__coverage__": "^11.0.0",
    "chai": "^3.5.0",
    "concurrently": "^2.0.0",
    "enzyme": "^2.2.0",
    "eslint": "^3.0.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-filenames": "^1.0.0",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "jsdom": "^9.4.1",
    "lodash": "^4.11.0",
    "mocha": "^2.4.5",
    "nyc": "^7.0.0",
    "react": "^15.0.1",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack-dev-server": "^1.14.1"
  },
  "peerDependencies": {
    "redux": "^3.0.0",
    "react": "^0.14.0 || ^15.0.0-0",
    "react-dom": "^0.14.0 || ^15.0.0-0"
  }
}
